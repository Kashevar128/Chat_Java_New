<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChatController">
      <VBox prefHeight="618.0" prefWidth="400.0">
         <TextArea fx:id="output" editable="false" prefHeight="600.0" prefWidth="400.0" />
         <HBox prefHeight="100.0" prefWidth="200.0">
               <TextField fx:id="input" onAction="#send" prefHeight="61.0" prefWidth="343.0" />
               <Button mnemonicParsing="false" onAction="s" prefHeight="72.0" prefWidth="58.0" text="SEND" />
         </HBox>
      </VBox>
</AnchorPane>
